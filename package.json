{
  "name": "its-market",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall_": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "ng serve --host 0.0.0.0",
    "start:stage": "ng serve --host 0.0.0.0 --configuration staging",
    "api": "nx serve api",
    "api:stage": "nx serve api --configuration staging",
    "admin": "nx serve admin --host 0.0.0.0",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "dev:ssr": "ng run its-market:serve-ssr",
    "serve:ssr": "node dist/its-market/server/main.js",
    "build:ssr": "ng build --prod && ng run its-market:server:production",
    "prerender": "ng run its-market:prerender",
    "nestjs-command": "CLI_PATH=./apps/api/src/cli.ts npx nestjs-command",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js -f ormconfig.js migration:run",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js -f ormconfig.js migration:create -n",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js -f ormconfig.js migration:generate -n",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js -f ormconfig.js migration:revert -n",
    "migration:show": "ts-node ./node_modules/typeorm/cli.js -f ormconfig.js migration:show -n",
    "schema:sync": "typeorm schema:sync",
    "schema:log": "typeorm schema:log",
    "schema:drop": "typeorm schema:drop",
    "query": "typeorm query",
    "cache:clear": "typeorm cache:clear",
    "nestjs:start": "node ./dist/apps/api/main.js",
    "generate:ormConf": "node ./dist/apps/api-command/main.js writeConfig:typeorm",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "lint-staged": {
    "*.{html,css,scss,js,ts,yml,yaml,md}": "prettier --write",
    "*.ts": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "nx affected:lint --parallel"
    }
  },
  "private": true,
